    },
    {
      "path": "dist/horus.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/glob": "^7.2.0",
    "eslint": "^8.10.0",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@types/stripe": "^8.0.417",
    "glob": "^7.2.0",
    "lodash": "^4.17.21"
  }
